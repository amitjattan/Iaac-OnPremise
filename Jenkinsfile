node {

    stage ('checkout') {

        echo "- - - - Checkout Code - - - -"
	checkout scm
    }

    stage ('setupenv') {
    	echo " - - - - Setting up environment now - - - - "
	}

    stage ('Vagrant') {
    
    echo " - - - - Ready to execute Vagrant now - - - - "
    cd "C:/Program Files/Git"
    ./git-bash.exe sh '''
    echo " - - - - - - C O M P L E T E D - - - - - - - "
    
    '''
    }
}
